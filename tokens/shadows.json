    },
    "xl": {
      "$type": "boxShadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "9px",
        "blur": "7px",
        "spread": "0",
        "color": "rgb(0 0 0 / 0.1)"
      }
    },
    "2xl": {
      "$type": "boxShadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "25px",
        "blur": "25px",
        "spread": "0",
        "color": "rgb(0 0 0 / 0.15)"
      }
    }
  }
}