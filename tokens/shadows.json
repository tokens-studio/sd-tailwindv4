{
  "shadow": {
    "2xs": {
      "$type": "boxShadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "1px",
        "blur": "0",
        "spread": "0",
        "color": "rgb(0 0 0 / 0.05)"
      }
    },
    "xs": {
      "$type": "boxShadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "1px",
        "blur": "2px",
        "spread": "0",
        "color": "rgb(0 0 0 / 0.05)"
      }
    },
    "sm": {
      "$type": "boxShadow",
      "$value": [
        {
          "offsetX": "0",
          "offsetY": "1px",
          "blur": "3px",
          "spread": "0",
          "color": "rgb(0 0 0 / 0.1)"
        },
        {
          "offsetX": "0",
          "offsetY": "1px",
          "blur": "2px",
          "spread": "-1px",
          "color": "rgb(0 0 0 / 0.1)"
        }
      ]
    },
    "md": {
      "$type": "boxShadow",
      "$value": [
        {
          "offsetX": "0",
          "offsetY": "4px",
          "blur": "6px",
          "spread": "-1px",
          "color": "rgb(0 0 0 / 0.1)"
        },
        {
          "offsetX": "0",
          "offsetY": "2px",
          "blur": "4px",
          "spread": "-2px",
          "color": "rgb(0 0 0 / 0.1)"
        }
      ]
    },
    "lg": {
      "$type": "boxShadow",
      "$value": [
        {
          "offsetX": "0",
          "offsetY": "10px",
          "blur": "15px",
          "spread": "-3px",
          "color": "rgb(0 0 0 / 0.1)"
        },
        {
          "offsetX": "0",
          "offsetY": "4px",
          "blur": "6px",
          "spread": "-4px",
          "color": "rgb(0 0 0 / 0.1)"
        }
      ]
    },
    "xl": {
      "$type": "boxShadow",
      "$value": [
        {
          "offsetX": "0",
          "offsetY": "20px",
          "blur": "25px",
          "spread": "-5px",
          "color": "rgb(0 0 0 / 0.1)"
        },
        {
          "offsetX": "0",
          "offsetY": "8px",
          "blur": "10px",
          "spread": "-6px",
          "color": "rgb(0 0 0 / 0.1)"
        }
      ]
    },
    "2xl": {
      "$type": "boxShadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "25px",
        "blur": "50px",
        "spread": "-12px",
        "color": "rgb(0 0 0 / 0.25)"
      }
    }
  },
  "inset-shadow": {
    "2xs": {
      "$type": "boxShadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "1px",
        "blur": "0",
        "spread": "0",
        "type": "innerShadow",
        "color": "rgb(0 0 0 / 0.05)"
      }
    },
    "xs": {
      "$type": "shadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "1px",
        "blur": "1px",
        "spread": "0",
        "color": "rgb(0 0 0 / 0.05)",
        "type": "innerShadow"
      }
    },
    "sm": {
      "$type": "boxShadow",
      "$value": {
        "offsetX": "0",
        "offsetY": "2px",
        "blur": "4px",
        "spread": "0",
        "color": "rgb(0 0 0 / 0.05)",
        "type": "innerShadow"
      }
    }
  },
  "drop-shadow": {
    "xs": {
      "$type": "boxShadow",
      "$value": {
